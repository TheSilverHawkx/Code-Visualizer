{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Project",
            "type": "npm",
            "script": "build",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$tsc",
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        },
        {
            "label": "Run Tests",
            "type": "npm",
            "script": "test",
            "group": "test",
            "problemMatcher": "$jest"
        }
    ]
}
